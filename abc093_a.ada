with Ada.Text_IO; use Ada.Text_IO;

procedure main is
   S : String(1..3);
   Last : Natural;
begin
   Get_Line(S, Last);

   if S(1) /= S(2) and S(2) /= S(3) and S(3) /= S(1) then
      Put_Line("Yes");
   else
      Put_Line("No");
   end if;
end main;
