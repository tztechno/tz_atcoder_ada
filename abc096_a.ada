with Ada.Text_IO;
use Ada.Text_IO;
with Ada.Integer_Text_IO;
use Ada.Integer_Text_IO;
procedure Main is
   a: Integer;
   b: Integer;
   X: Integer;
begin
   Get(a);
   Get(b); 
   if a<=b then
      X := a;
   else
      X := a-1;
   end if;
   Put(X, 0);
   New_Line;
end Main;
