with Ada.Text_IO; use Ada.Text_IO;
with Ada.Integer_Text_IO; use Ada.Integer_Text_IO;
procedure Main is
   n: Integer;
   a: Integer;
   b: Integer;
begin
   Get(n);
   if n = 1 then
      Put_Line("Hello World");
   else
      Get(a);
      Get(b);
      Put(a + b, 0);
   end if;
end Main;
