with Ada.Text_IO;use Ada.Text_IO;
with Ada.Integer_Text_IO;use Ada.Integer_Text_IO;
procedure Main is
   X: Integer;
begin
   Get(X);
   if (X-7)*(X-5)*(X-3)=0 then
   Put("YES");
   else
   Put("NO");
   end if;
   New_Line;
end Main;
